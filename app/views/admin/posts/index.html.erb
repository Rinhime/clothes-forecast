      <%= @user.name%>さんのページ
      <table cellpadding="10">
        <thead>
          <tr>
            <th>気温</th>
            <th>投稿内容</th>
            <th>投稿日</th>
          </tr>
        </thead>
        <% @posts.each do |post| %>
        <tbody>
          <tr>
            <td><%= post.clothe.celsius %></td>
            <td width="350px"><%= post.text %></td>
            <td><%= post.created_at.strftime('%Y/%m/%d') %></td>
            <td><%= link_to "削除する", public_post_path(post.id), method: :delete, "data-confirm" => "本当に削除しますか？" %></td>
          </tr>
        </tbody>
        <% end %>
      </table>